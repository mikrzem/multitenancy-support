<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.VBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>
<VBox
        xmlns="http://javafx.com/javafx"
        xmlns:fx="http://javafx.com/fxml"
        fx:controller="pl.net.gwynder.multitenency.support.configuration.controllers.ServerConfigurationGroupEditTreeController"
        prefHeight="400.0" prefWidth="600.0">
    <TreeView fx:id="editTree"/>
    <Button fx:id="newRootGroup">
        <graphic>
            <FontIcon iconLiteral="fa-plus-square"/>
        </graphic>
        Add new root group
    </Button>
</VBox>
